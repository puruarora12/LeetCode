class Solution {
    public boolean isPowerOfThree(int n) {
        return Integer.toString(n,3).matches("^10*$");
    }
}

/////////////////////////////////////////////////////////

class Solution {
    public boolean isPowerOfThree(int n) {
        return (Math.log10(n)/Math.log10(3))%1==0;
    }
}

/////////////////////////////////////////////////////////

class Solution {
    public boolean isPowerOfThree(int n) {
        return (n>0 && Math.pow(3,19)%n==0) ;
    }
}
